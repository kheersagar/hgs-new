{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./build",
      "dest": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "./build",
      "dest": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "./index.js"
    }
  ]
}
